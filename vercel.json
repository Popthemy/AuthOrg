{
  "version": 2,
  "builds": [
    {
      "src": "AuthOrg/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "AuthOrg/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "postInstallCommand": "python manage.py migrate"
}
