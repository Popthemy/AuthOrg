{
  "version": 2,
  "builds": [
    {
      "src": "AuthOrg/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "env": {
          "DJANGO_SETTINGS_MODULE": "AuthOrg.settings.prod"
        }
      }
    },
    {
      "src": "build_files.sh",  
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "AuthOrg/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
